<Page x:Class="DontHarmWPF.Pages.AdminPages.MainMenuPage"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
      xmlns:local="clr-namespace:DontHarmWPF.Pages.AdminPages"
      mc:Ignorable="d"
      Title="Меню администратора" Height="450" Width="800">
    <Grid>
        <StackPanel Orientation="Vertical"
                    VerticalAlignment="Center"
                    HorizontalAlignment="Center">
            <Button 
                    Content="Сформировать отчет"/>
            <Button x:Name="HistoryButton"
                    Click="HistoryButton_OnClick"
                    Content="История входов"/>
            <Button 
                    Content="Информация о расходниках"/>
        </StackPanel>
    </Grid>
</Page>
